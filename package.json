    "awesome-typescript-loader": "3.2.3",
    "codecov.io": "^0.1.6",
    "codelyzer": "^3.0.0-beta.4",
    "commitizen": "^2.9.5",
    "core-js": "2.5.0",
    "css-loader": "0.28.5",
    "cz-conventional-changelog": "^2.0.0",
    "ghooks": "^2.0.0",
    "gulp": "3.9.1",
    "gulp-inline-ng2-styles": "0.0.1",
    "gulp-inline-ng2-template": "4.0.0",
    "gulp-rimraf": "0.2.1",
