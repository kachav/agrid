{
  "name": "agrid",
  "version": "0.0.0-development",
  "description": "an angular 2 data grid",
  "main": "index.js",
  "scripts": {
    "check-coverage": "nyc check-coverage --statements 90 --branches 90 --functions 100 --lines 100",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "server": "webpack-dev-server",
    "prebuild": "gulp cleanLib",
    "build": "gulp build",
    "test": "karma start",
    "test:single": "karma start --single-run",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "'Alexander <'kachesov.alexander@live.com'>",
  "license": "ISC",
  "peerDependencies": {
    "@angular/common": ">=4.0.0",
    "@angular/compiler": ">=4.0.0",
    "@angular/core": ">=4.0.0"
  },
  "dependencies": {
    "agroupfor": "1.1.2"
  },
  "devDependencies": {
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@types/core-js": "0.9.43",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.2.34",
    "@types/lodash": "4.14.74",
